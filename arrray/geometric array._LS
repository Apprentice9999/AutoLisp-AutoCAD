
(defun c:g2()
  (setq line (entsel))
  (setq a0 (car (cadr line)))
  (setq a1 (car (getpoint "2nd Line")))
  (setq r (/ a1 a0))
  (setq end (car (getpoint "End")))
  (setq n 1)
  (setq x (* a0 (expt r (- n 1))))
  (print a0)
  (print a1)
  (print r)
  (print (expt (- end x) 2))
  (while (> (expt (- end x) 2) 2)
    	(setq x (* a0 (expt r (- n 1))))
    	(print (expt (- end x) 2))
	(command "_.offset" x line "1" "")
    	(setq n (+ n 1))
    )

  )